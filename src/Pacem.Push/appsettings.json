{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "{YOUR_CONNECTIONSTRING_HERE}"
  },
  "Vapid": {
    "Subject": "{YOUR_SUBJECT_HERE}",
    "PublicKey": "{YOUR_VAPID_PUBLIC_KEY_HERE}",
    "PrivateKey": "{YOUR_VAPID_PRIVATE_KEY_HERE}"
  },
  "OAuth2": {
    "Authority": "{YOUR_IDENTITYSERVER_URL_HERE}",
    "ClientId": "{YOUR_CLIENT_ID_HERE}",
    "ClientSecret": "{YOUR_CLIENT_SECRET_HERE}"
  }
}
